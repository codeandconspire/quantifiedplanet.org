{
  "name": "quantifiedplanet.org",
  "version": "2.2.5",
  "description": "Website for Quantified Planet",
  "main": "index.js",
  "scripts": {
    "test": "standard",
    "start": "node server.js"
  },
  "documentify": {
    "transform": [
      [
        "./lib/document.js",
        {
          "order": "end"
        }
      ]
    ]
  },
  "now": {
    "alias": "next.quantifiedplanet.org",
    "env": {
      "NODE_ENV": "production",
      "PRISMIC_QUANTIFIEDPLANET_SECRET": "@prismic-quantifiedplanet-secret",
      "CLOUDFLARE_QUANTIFIEDPLANET_ZONE": "@cloudflare-quantifiedplanet-zone",
      "CLOUDFLARE_CODEANDCONSPIRE_EMAIL": "@cloudflare-codeandconspire-email",
      "CLOUDFLARE_CODEANDCONSPIRE_KEY": "@cloudflare-codeandconspire-key"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeandconspire/quantifiedplanet.org.git"
  },
  "author": "codeandconspire",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/codeandconspire/quantifiedplanet.org/issues"
  },
  "homepage": "https://github.com/codeandconspire/quantifiedplanet.org#readme",
  "devDependencies": {
    "standard": "^10.0.3"
  },
  "dependencies": {
    "cccpurge": "^1.2.2",
    "chartist": "^0.11.0",
    "choo": "^6.12.1",
    "choo-devtools": "^2.3.3",
    "choo-service-worker": "^2.4.0",
    "cssnano": "^3.10.0",
    "dedent": "^0.7.0",
    "dotenv-extended": "^2.0.2",
    "hstream": "^1.2.0",
    "jalla": "^0.3.5",
    "koa-body": "^4.0.3",
    "koa-compose": "^4.1.0",
    "koa-route": "^3.2.0",
    "nanobus": "^4.3.2",
    "nanoraf": "^3.0.1",
    "nanostate": "^1.1.0",
    "normalize.css": "^7.0.0",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.2.0",
    "postcss-load-config": "^1.2.0",
    "postcss-selector-matches": "^3.0.1",
    "prismic-element": "^1.0.0",
    "prismic-javascript": "^1.5.0-beta.1",
    "prismic-richtext": "^0.2.7",
    "smoothscroll-polyfill": "^0.4.0"
  }
}
